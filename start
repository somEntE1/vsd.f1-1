import telebot
from telebot import types

API_TOKEN = '7845888397:AAHEgo4-oT8FdzZJ52O45itGEANH3l46ZwU'

bot = telebot.TeleBot(API_TOKEN)

# Обработчик команды /start
@bot.message_handler(commands=['start'])
def send_welcome(message):
    markup = types.InlineKeyboardMarkup()
    
    # Создайте кнопки с ссылками
    button1 = types.InlineKeyboardButton("Ссылка 1", url="https://example.com/1")
    button2 = types.InlineKeyboardButton("Ссылка 2", url="https://example.com/2")
    button3 = types.InlineKeyboardButton("Ссылка 3", url="https://example.com/3")
    
    # Добавляем кнопки на клавиатуру
    markup.add(button1, button2, button3)
    
    bot.send_message(message.chat.id, "Привет! Выберите ссылку:", reply_markup=markup)

# Запускаем бота
if __name__ == '__main__':
    bot.polling(none_stop=True)
